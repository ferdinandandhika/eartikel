{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "api/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/api/(.*)\\.php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/(.*)\\.php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "POSTGRES_HOST": "db.qecotzvnfguaxaahnanu.supabase.co",
    "POSTGRES_USER": "postgres",
    "POSTGRES_PASSWORD": "P4EizpXzzThDXIII",
    "POSTGRES_DATABASE": "postgres",
    "POSTGRES_PORT": "5432",
    "PHP_VERSION": "8.2"
  }
}